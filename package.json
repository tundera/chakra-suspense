{
  "name": "chakra-suspense",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@7.0.0-beta.2",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "commit": "gacp",
    "prepare": "husky install"
  },
  "dependencies": {
    "@chakra-ui/react": "^1.8.7",
    "@chakra-ui/theme-tools": "^1.3.6",
    "@emotion/react": "^11.8.2",
    "@emotion/styled": "^11.8.1",
    "framer-motion": "^6.2.8",
    "next": "12.1.3",
    "react": "18.0.0",
    "react-dom": "18.0.0",
    "react-icons": "^4.3.1"
  },
  "devDependencies": {
    "@chakra-ui/cli": "^1.9.0",
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@testing-library/react": "^12.1.4",
    "@types/eslint": "^8.4.1",
    "@types/node": "17.0.23",
    "@types/prettier": "^2.4.4",
    "@types/react": "17.0.43",
    "@types/react-dom": "17.0.14",
    "@vitejs/plugin-react": "^1.3.0",
    "c8": "^7.11.0",
    "cypress": "^9.5.3",
    "eslint": "8.12.0",
    "eslint-config-next": "12.1.2",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-chakra-ui": "^0.6.3",
    "eslint-plugin-prettier": "^4.0.0",
    "gacp": "^2.10.2",
    "husky": "^7.0.4",
    "jsdom": "^19.0.0",
    "lint-staged": "^12.3.7",
    "prettier": "^2.6.1",
    "pretty-quick": "^3.1.3",
    "typescript": "4.6.3",
    "vite-tsconfig-paths": "^3.4.1",
    "vitest": "^0.8.1"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@babel/core",
        "@chakra-ui/system",
        "vite"
      ],
      "allowedVersions": {
        "react": "*",
        "react-dom": "*"
      }
    }
  },
  "gacp": {
    "emoji": "emoji"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
