{
  "name": "chakra-suspense",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@7.0.0-rc.2",
  "prisma": {
    "schema": "db/schema.prisma",
    "seed": "esno -r tsconfig-paths/register scripts/seed.ts"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development binode --require ./mocks -- next dev",
    "build": "next build",
    "start": "next start",
    "start:mocks": "binode --require ./mocks -- next start",
    "lint": "TIMING=1 next lint",
    "typecheck": "tsc --noEmit",
    "theme": "chakra-cli tokens src/styles/theme/index.ts --out node_modules/@chakra-ui/styled-system/dist/declarations/src/theming.types.d.ts",
    "test": "vitest",
    "test:e2e:dev": "start-server-and-test dev http://localhost:3000 \"cypress open\"",
    "pretest:e2e:run": "pnpm build",
    "test:e2e:run": "cross-env PORT=8811 start-server-and-test start:mocks http://localhost:8811 \"cypress run\"",
    "commit": "gacp",
    "prepare": "husky install && pnpm theme"
  },
  "dependencies": {
    "@chakra-ui/react": "1.8.7",
    "@chakra-ui/styled-system": "1.19.0",
    "@chakra-ui/theme-tools": "1.3.6",
    "@emotion/react": "11.8.2",
    "@emotion/styled": "11.8.1",
    "@next-auth/prisma-adapter": "1.0.3",
    "@prisma/client": "3.11.1",
    "framer-motion": "6.2.8",
    "next": "12.1.5-canary.0",
    "next-auth": "4.3.1",
    "react": "18.0.0",
    "react-dom": "18.0.0",
    "react-error-boundary": "3.1.4",
    "react-icons": "4.3.1"
  },
  "devDependencies": {
    "@chakra-ui/cli": "1.9.0",
    "@commitlint/cli": "16.2.3",
    "@commitlint/config-conventional": "16.2.1",
    "@faker-js/faker": "6.1.1",
    "@fontsource/fira-code": "4.5.8",
    "@fontsource/inter": "4.5.7",
    "@testing-library/cypress": "8.0.2",
    "@testing-library/react": "13.0.0-alpha.7",
    "@testing-library/react-hooks": "8.0.0-alpha.1",
    "@testing-library/user-event": "14.0.4",
    "@types/eslint": "8.4.1",
    "@types/node": "17.0.23",
    "@types/prettier": "2.4.4",
    "@types/react": "17.0.43",
    "@types/react-dom": "17.0.14",
    "@vitejs/plugin-react": "1.3.0",
    "binode": "1.0.5",
    "c8": "7.11.0",
    "cross-env": "7.0.3",
    "cypress": "9.5.3",
    "eslint": "8.12.0",
    "eslint-config-next": "12.1.2",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-chakra-ui": "0.6.3",
    "eslint-plugin-prettier": "4.0.0",
    "esno": "0.14.1",
    "gacp": "2.10.2",
    "husky": "7.0.4",
    "jsdom": "19.0.0",
    "lint-staged": "12.3.7",
    "msw": "0.39.2",
    "prettier": "2.6.2",
    "pretty-quick": "3.1.3",
    "prisma": "3.11.1",
    "start-server-and-test": "1.14.0",
    "tasuku": "1.0.2",
    "ts-node": "10.7.0",
    "tsconfig-paths": "3.14.1",
    "typescript": "4.6.3",
    "vite-tsconfig-paths": "3.4.1",
    "vitest": "0.8.2"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@babel/core",
        "@chakra-ui/system",
        "@testing-library/dom",
        "vite"
      ],
      "allowedVersions": {
        "react": "*",
        "react-dom": "*"
      }
    }
  },
  "gacp": {
    "emoji": "emoji"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "msw": {
    "workerDirectory": "public"
  }
}
